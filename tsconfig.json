{
  "compilerOptions": {
    "incremental": true,
    "target": "ES2016",
    "jsx": "preserve",
    "lib": ["ES2016", "DOM"],
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "paths": {
      "@/*": ["./src/*"],
      "~/*": ["./src/*"],
      "src/*": ["./src/*"]
    },
    "resolveJsonModule": true,
    "types": [
      "unplugin-icons/types/vue",
      "webextension-polyfill",
      "chrome-types",
      "chrome",
    ],
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "skipLibCheck": true
  },
  "include": [
    "package.json",
    "vite.config.ts",
    // "manifest.config.ts",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
//    "node_modules/.pnpm/unplugin-auto-import@0.18.2_@vueuse+core@10.11.0/node_modules/unplugin-auto-import/auto-imports.d.ts"
  ]
  // "references": [
  //   {
  //     "path": "./tsconfig.node.json"
  //   }
  // ]
}
